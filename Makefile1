OBJS	= hopscotch.o partition.o bitmap.o partitionedHashJoin.o list.o utilities.o 
SOURCE	= hopscotch.c partition.c bitmap.c partitionedHashJoin.c list.cutilities.c 
HEADER	= hopscotch.h bitmap.h mainPartitionTest.h list.h
OUT	= test
CC	 = gcc
FLAGS	 = -g -c -Wall
LFLAGS	 = -lm

all: $(OBJS)
	$(CC) -g $(OBJS) -o $(OUT) $(LFLAGS)

hopscotch.o: hopscotch.c
	$(CC) $(FLAGS) hopscotch.c 

# hopscotch_test.o: hopscotch_test.c
# 	$(CC) $(FLAGS) hopscotch_test.c 

bitmap.o: bitmap.c
	$(CC) $(FLAGS) bitmap.c 

partition.o: partition.c
	$(CC) $(FLAGS) partition.c

partitionedHashJoin.o: partitionedHashJoin.c
	$(CC) $(FLAGS) partitionedHashJoin.c

list.o: list.c
	$(CC) $(FLAGS) list.c

utilities.o: utilities.c
	$(CC) $(FLAGS) utilities.c 

mainPartitionTest.o: mainPartitionTest.c
	$(CC) $(FLAGS) mainPartitionTest.c

clean:
	rm -f $(OBJS) $(OUT)

run: $(OUT)
	./$(OUT)